/dts-v1/;

/include/ "tegra20.dtsi"

/ {
	model = "Compulab TrimSlice board";
	compatible = "compulab,trimslice", "nvidia,tegra20";

	memory@0 {
		reg = < 0x00000000 0x40000000 >;
	};

	i2c@7000c000 {
		clock-frequency = <400000>;
	};

	i2c@7000c400 {
		clock-frequency = <400000>;
	};

	i2c@7000c500 {
		clock-frequency = <400000>;
	};

	serial@70006000 {
		clock-frequency = < 216000000 >;
	};

	sdhci@c8000600 {
		cd-gpios = <&gpio 121 0>;
		wp-gpios = <&gpio 122 0>;
	};
};
